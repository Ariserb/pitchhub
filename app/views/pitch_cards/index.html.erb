<p id="notice"><%= notice %></p>

<h1>Listing Pitch Cards</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pitch_cards.each do |pitch_card| %>
      <tr>
        <td><%= link_to 'Show', pitch_card %></td>
        <td><%= link_to 'Edit', edit_pitch_card_path(pitch_card) %></td>
        <td><%= link_to 'Destroy', pitch_card, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pitch card', new_pitch_card_path %>
